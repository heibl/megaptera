	Journal = {Revista Chilena de Historia Natural},
	Keywords = {Atacama, ecophysiology, fog},
	Pages = {737-749},
	Title = {Evaluaci{\'o}n de la capacidad estacional de utilizar eventos de precipitaci{\'o}n en tres especies de arbustos nativos de {Chile} con distintos sistemas radiculares},
